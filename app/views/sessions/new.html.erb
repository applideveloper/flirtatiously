<h1>Sign In</h1>

<form action="<%= session_url%>" class="signup" method="post">
  <%= auth_token_input %>

  <label>

    <input type="text" name="user[username]" placeholder="Username">
  </label>

  <label>

    <input type="password" name="user[password]" placeholder="Password">
  </label>

  <input type="submit" value="Sign In">
</form>