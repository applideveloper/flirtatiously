<li class="object-item group">
  <% profile = Profile.find(status_message.p_id) %>
  <div class="object-avatar">

    <img src="<%= profile.photos.first.file.url(:avatar) %>" height="100%" width="100%">
  </div>
  <section class="object-content">
    <a class="username" href="<%= profile_url(profile) %>">
      <%= status_message.username %>
    </a>
    <div class="status-body">
      <%= status_message.body %>

    </div>

    <div class="status-timestamp">
      <%= status_message.created_at %>
    </div>
  </section>
</li>